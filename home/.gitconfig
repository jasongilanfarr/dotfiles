[color]
   ui = auto
   branch = auto
   status = auto
   diff = auto
[color "branch"]
   current = yellow reverse
   local = yellow
   remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
   added = yellow
   changed = green
   untracked = cyan
[alias]
   st = status
   br = branch
   co = checkout
   ca = commit -a --amend -C HEAD
   t = tag
   git = !exec git
   ru = review update
   r = review
   rc = review create -ag --guess-desc -o --no-prompt
   rup = review update --parent
   rcp = review create -ag --guess-desc -o --no-prompt --parent
   sa = submit --async
   uc = !$HOME/src/update-commit.sh
   np = !exec git --no-pager
   acl = review status --verify-diff
[merge]
   keepBackup = false
   renamelimit = 21000
[core]
	editor = /usr/bin/vim
[push]
	default = current
	autoSetupRemote = true
